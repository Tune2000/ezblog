<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.easy.mapper.TagMapper">
    <update id="myTagUpdateById">
        UPDATE
            `ez_tag` tag
        SET
            tag.`del_flag` = 1
        WHERE
            tag.`id`=#{id}
    </update>
</mapper>